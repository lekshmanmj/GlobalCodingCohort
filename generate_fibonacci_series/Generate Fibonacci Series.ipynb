   "source": [
    "Problem:\n",
    "\n",
    "Generate n number of fibonacci series, where 1 <= n <= 100\n",
    "\n",
    "Samples:\n",
    "\n",
    "Input 1:\n",
    "n = 1\n",
    "\n",
    "Output 1:\n",
    "0\n",
    "\n",
    "Input 2:\n",
    "n = 5\n",
    "\n",
    "Output 2:\n",
    "0\n",
    "1\n",
    "1\n",
    "2\n",
    "3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "34ed03ca-2221-4294-9a91-217713ceecb2",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0\n"
     ]
    }
   ],
   "source": [
    "# solution\n",
    "n = 1\n",
    "\n",
    "second_last = 0 \n",
    "last = 1\n",
    "\n",
    "if n == 1: print(second_last)\n",
    "elif n >= 2:\n",
    "    print(second_last)\n",
    "    print(last)\n",
    "    \n",
    "for i in range(n-2):\n",
    "    new = second_last + last\n",
    "    second_last = last\n",
    "    last = new\n",
    "    print(last)"
   ]
  },
